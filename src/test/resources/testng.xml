<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Tealium E-commerce Automation Test Suite" verbose="1">

    <listeners>
        <listener class-name="listeners.TestListener"/>
    </listeners>

    <test name="Account Tests">
        <classes>
            <class name="tests.AuthenticationTests"/>
        </classes>
    </test>

    <test name="E-commerce Tests">
        <classes>
            <class name="tests.EcommerceTests"/>
        </classes>
    </test>

</suite>
